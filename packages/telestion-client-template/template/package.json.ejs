{
	"name": "<%- moduleName %>",
	"description": "<%- projectName %>, a Telestion PSC generated using the telestion-client-cli",
	"version": "0.0.0",
	"homepage": ".",
	"scripts": {
		"prepare": "husky install",
		"start": "tc-cli start",
		"start:electron": "tc-cli start electron",
		"start:browser": "tc-cli start browser",
		"build": "tc-cli build",
		"stats": "tc-cli stats",
		"style": "prettier --write .",
		"style:ci": "prettier --list-different .",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-pattern node_modules/ --no-error-on-unmatched-pattern src",
		"test:e2e": "concurrently --success last --kill-others pnpm:test:e2e:serve pnpm:test:e2e:cypress",
		"test:e2e:serve": "tc-cli build --no-electron && serve --no-request-logging -p 8080 dist",
		"test:e2e:cypress": "wait-on 'http://localhost:8080' && cypress run",
		"check": "tsc",
		"clean": "rimraf dist release"
	},
	"targets": {
		"electron": {
			"source": "src/electron.ts",
			"context": "electron-main",
			"distDir": "dist"
		},
		"frontend": {
			"source": "src/index.html",
			"context": "browser",
			"distDir": "dist",
			"engines": {
				"browsers": "last 2 Chrome versions"
			},
			"publicUrl": "./"
		}
	},
	"private": true,
	"electronDependencies": <%- electronDependencies %>,
	"dependencies": <%- dependencies %>,
	"devDependencies": <%- devDependencies %>
}
